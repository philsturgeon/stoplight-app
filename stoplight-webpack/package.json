{
  "name": "reactjs-stoplight",
  "version": "1.0.0",
  "description": "React Stoplight",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --port 3002",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Sunitta",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "style-loader": "^2.0.0",
    "webpack": "^1.13.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@stoplight/elements": "^6.4.1",
    "@stoplight/http-spec": "^4.2.0",
    "@stoplight/http-ui": "^2.1.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
